<h1> New Test Form</h1>
<h4> This is a form test</h4>

<%= form_for @changeset, topic_path(@conn, :create), fn f -> %>
    <div class="form-grouo">
        <%= text_input f, :title, placeholder: "Please enter a new Topic Title", class: "form-control" %>
        <%= error_tag f, :title %>
    </div>

    <%= submit "Save Topic", class: "btn btn-primery" %>
<% end %>

